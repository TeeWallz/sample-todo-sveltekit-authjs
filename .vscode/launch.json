{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Vite Dev Server",
            "runtimeExecutable": "npx",
            "runtimeArgs": ["vite", "--port=4069"],
            "cwd": "${workspaceFolder}",
            "protocol": "inspector",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopOnEntry": false,
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "env": {
                "FORCE_COLOR": "3"
            }
        }
    ]
}
